<?xml version="1.0" encoding="utf-8" ?>
<ruleset name="PHP 8.0 Compatibility">
    <description>The PHP 8.0 compatibility inspection.</description>

    <file>../../src</file>
    <file>../../tests</file>
    <arg name="extensions" value="php,html"/>
    <arg name="basepath" value="../../"/>
    <arg name="colors" />
    <arg name="parallel" value="1" />
    <arg value="p"/>
    <config name="testVersion" value="8.0"/>
    <rule ref="PHPCompatibility"/>
</ruleset>
